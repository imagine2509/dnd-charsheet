{
    "proxy": "http://localhost:5173/",
    "name": "dnd-charsheet",
    "version": "1.0.0",
    "description": "",
    "main": "app.js",
    "scripts": {
        "start": "node app.js",
        "dev": "nodemon app.js --ignore ./sessions --ext js,jsx,json",
        "db-seed": "npx sequelize-cli db:seed:all",
        "db-seed-undo": "npx sequelize db:seed:undo:all",
        "db-create": "npx sequelize db:create",
        "db-drop": "npx sequelize db:drop",
        "db-migrate": "npx sequelize db:migrate:undo:all && npx sequelize",
        "db-user": "npx sequelize-cli model:generate --name User --attributes email:string,password:string,login:string,firstName:string,lastName:string,activationString:string,isAdmin:boolean",
        "db-token": "npx sequelize-cli model:generate --name Token --attributes userId:integer,refreshToken:string"
    },
    "keywords": [],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "bcrypt": "^5.1.0",
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-validator": "^7.0.1",
        "jsonwebtoken": "^9.0.2",
        "morgan": "^1.10.0",
        "nodemailer": "^6.9.7",
        "pg": "^8.11.3",
        "pg-hstore": "^2.3.4",
        "sequelize": "^6.32.1",
        "sequelize-cli": "^6.6.1"
    },
    "devDependencies": {
        "eslint": "^8.45.0",
        "eslint-config-airbnb": "^19.0.4",
        "eslint-config-airbnb-base": "^15.0.0",
        "eslint-plugin-import": "^2.27.5",
        "eslint-plugin-jsx-a11y": "^6.7.1",
        "eslint-plugin-react": "^7.33.0",
        "eslint-plugin-react-hooks": "^4.6.0",
        "nodemon": "^3.0.1"
    }
}
